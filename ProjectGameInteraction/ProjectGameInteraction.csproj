<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Afbeeldingen\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Remove="Afbeeldingen\3885422.png" />
    <None Remove="Afbeeldingen\alarmclock.png" />
    <None Remove="Afbeeldingen\citybackground.png" />
    <None Remove="Afbeeldingen\exitbutton.png" />
    <None Remove="Afbeeldingen\helderheid.png" />
    <None Remove="Afbeeldingen\logo klant.png" />
    <None Remove="Afbeeldingen\music.png" />
    <None Remove="Afbeeldingen\playbutton.png" />
    <None Remove="Afbeeldingen\playbutton2.png" />
    <None Remove="Afbeeldingen\returnicon.png" />
    <None Remove="Afbeeldingen\settings.png" />
    <None Remove="Afbeeldingen\shoppingcart.png" />
    <None Remove="Afbeeldingen\volume.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Afbeeldingen\3885422.png" />
    <Resource Include="Afbeeldingen\alarmclock.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\citybackground.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\exitbutton.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\helderheid.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\music.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\playbutton.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\playbutton2.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\returnicon.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\settings.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\shoppingcart.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Afbeeldingen\volume.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <Resource Include="Afbeeldingen\logo klant.png" />
  </ItemGroup>

</Project>
